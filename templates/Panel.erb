Ext.define('<%=@full_path%>', {
    extend: 'Ext.panel.Panel',
    title: '',
    alias: 'widget.<%=@element_alias%>',
    bodyPadding: '10px',
    border: false,
    layout: {
        type: '<%=@eq_options[:layout] or 'anchor'%>',
        align: 'stretch'
    },
    <% if option? '-c' %>
    constructor: function (cfg) {
        cfg = cfg || {}
        var me = this
        me.items = []
        me.callParent(arguments)
    }
    <% else %>
    items: []
    <% end %>
})
